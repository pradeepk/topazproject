<!DOCTYPE rdf:RDF [
<!ENTITY rdf       "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
<!ENTITY rdfs      "http://www.w3.org/2000/01/rdf-schema#">
<!ENTITY owl       "http://www.w3.org/2002/07/owl#">
<!ENTITY xsd       "http://www.w3.org/2001/XMLSchema#">

<!ENTITY dcterms   "http://purl.org/dc/terms/">
<!ENTITY dctype    "http://purl.org/dc/dcmitype/">
<!ENTITY dc        "http://purl.org/dc/elements/1.1/">

<!ENTITY topaz     "http://rdf.topazproject.org/RDF/">
<!ENTITY ambra     "http://rdf.plos.org/RDF/">
]>

<rdf:RDF xmlns="http://www.topazproject.org/ambra#" xml:base="http://www.topazproject.org/ambra#"
  xmlns:dcterms="&dcterms;"
  xmlns:dctype="&dctype;"
  xmlns:dc="&dc;"
  xmlns:topaz="&topaz;"
  xmlns:ambra="&ambra;"
  xmlns:xsd="&xsd;"
  xmlns:owl="&owl;"
  xmlns:rdf="&rdf;"
  xmlns:rdfs="&rdfs;">

<owl:Ontology rdf:about="http://www.topazproject.org/ambra#"/>

<owl:ObjectProperty rdf:about="&topaz;objectSize">
  <rdfs:subPropertyOf rdf:resource="&dcterms;extent"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="&topaz;contentType">
  <rdfs:subPropertyOf rdf:resource="&dcterms;format"/>
</owl:ObjectProperty>

</rdf:RDF>

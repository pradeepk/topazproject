<config>
  <pub>
    <app-context>/plosone-webapp</app-context>
    <!-- virtual journal config -->
    <virtualJournals>
      <!-- default values used if no virtual journal rules match -->
      <default>
        <journal>PLoSONE</journal>
        <description>PLoSONE</description>
        <mappingPrefix></mappingPrefix>
        <url>http://plosone...</url>
      </default>
      <!-- entries to config each virtual journal -->
      <!-- comma separated list of virtual journal names -->
      <journals>PLoSClinicalTrials</journals>
      <PLoSClinicalTrials>
        <journal>PLoSClinicalTrials</journal>
        <description>PloS Clinical Trials</description>
        <mappingPrefix>/journals/clinicalTrials</mappingPrefix>
        <rules>
          <host>clinicaltrials.*</host>
        </rules>
        <url>http://clinicaltrials...</url>
        <pub>
          <feed>
            <title>PLoS Clinical Trials</title>
            <id>info:doi/10.1371/feed.phct</id>
          </feed>
        </pub>
      </PLoSClinicalTrials>
    </virtualJournals>
  </pub>
  <dummy>
    <sso>
      <enabled>true</enabled>
    </sso>
  </dummy>
